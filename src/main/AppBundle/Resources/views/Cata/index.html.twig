{% extends '::base.html.twig' %}
{% block body %}

<div class="notice">
    {% for message in app.session.flashBag.get('notice') %}
        <div class="flash-notice">
            {{ message }}
        </div>
    {% endfor %}
</div>

{{ form_start(form) }}
    <div>Marque:</div>
    <div>
       {{ form_errors(form) }}
       {{ form_widget(form) }}
    </div>

{{ form_end(form) }}
{% endblock %}
